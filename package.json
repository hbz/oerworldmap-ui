{
  "name": "oerworldmap-ui",
  "version": "0.0.1",
  "description": "The user interface for https://oerworldmap.org/",
  "scripts": {
    "clean": "rimraf dist/",
    "build:dev": "NODE_ENV=development webpack-cli -w",
    "build:prod": "NODE_ENV=production webpack-cli",
    "server:dev": "NODE_ENV=development nodemon -w dev dev/server.js",
    "server:prod": "NODE_ENV=production node dist/server.js",
    "test": "eslint src --quiet && jest",
    "singleTest": "node --inspect-brk  node_modules/.bin/jest --verbose=false --runInBand -t ",
    "fix": "eslint src --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint src --ext .jsx,.js --quiet",
    "lint:style": "stylelint 'src/styles/**/*.pcss'",
    "site:dev": "NODE_ENV=development webpack-dev-server --config site.config.babel.js",
    "site:prod": "NODE_ENV=production webpack-cli --config site.config.babel.js",
    "storybook": "start-storybook",
    "build:storybook": "build-storybook -c .storybook -o .out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbz/oerworldmap-ui.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
